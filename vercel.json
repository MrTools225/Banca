{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "@vercel/php"
        },
        {
            "src": "auth/**/*.php",
            "use": "@vercel/php"
        },
        {
            "src": "admin/**/*.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}